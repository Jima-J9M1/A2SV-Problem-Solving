class Solution:

    def maxProductDifference(self, nums: List[int]) -> int:

        n = len(nums)

        nums.sort()

        num1 = nums[0]

        num2 = nums[1]

        num3 = nums[n-1]

        num4 = nums[n-2]

        result = ((num3 * num4) - (num1 * num2))

        

        return result
